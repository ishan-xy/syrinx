script/source = "extends CharacterBody2D

# Player's movement speed
const SPEED = 100 * 60

# Player States
enum Direction { right, up, left, down }
@export var movement := Direction.down
@onready var anim := $AnimatedSprite2D
@onready var camera := $Camera2D
var isInteracting = false

# Custom movement logic for 4 (NOT `8`) directional movement
var _prev := PackedByteArray()

func _ready():
	camera.make_current()

func pressed_direction(direction: Direction) -> void:
	_prev.append(direction)
	movement = direction
	anim.animation_type = anim.AnimationType.walk
	update_animation_type(anim.AnimationType.walk)

func released_direction(direction: Direction) -> void:
	var pos := _prev.rfind(direction)
	if pos != -1:
		_prev.remove_at(pos)
	if _prev.is_empty():
		anim.animation_type = anim.AnimationType.idle
		update_animation_type(anim.AnimationType.idle)
		return
	else:
		movement = _prev[-1] as Direction
	return

func update_animation_type(type: int) -> void:
	anim.animation_type = type

func process_ui_key(direction: Direction) -> bool:
	var key_name: String = \"move_\" + Direction.keys()[direction]
	if Input.is_action_just_pressed(key_name):
		pressed_direction(direction)
		return true
	elif Input.is_action_just_released(key_name):
		released_direction(direction)
		return true
	return false

# Process user input
func process_input() -> void:
	var inp: bool = false
	inp = inp or process_ui_key(Direction.right)
	inp = inp or process_ui_key(Direction.up)
	inp = inp or process_ui_key(Direction.left)
	inp = inp or process_ui_key(Direction.down)
	if Input.is_key_pressed(KEY_SHIFT):
		if anim.animation_type == anim.AnimationType.walk:
			anim.animation_type = anim.AnimationType.sprint
			update_animation_type(anim.AnimationType.sprint)
	if inp:
		$Direction.set_rotation_degrees(90 * movement)
		update_rotation_degrees(90 * movement)

# Process input and move
func _physics_process(delta: float) -> void:
	if not isInteracting:
		process_input()
		if not (Input.is_action_pressed(\"move_up\") or Input.is_action_pressed(\"move_right\") or \\
				Input.is_action_pressed(\"move_down\") or Input.is_action_pressed(\"move_left\")):
			anim.animation_type = anim.AnimationType.idle
			update_animation_type(anim.AnimationType.idle)

		if anim.animation_type == anim.AnimationType.idle:
			return

		velocity.x = SPEED if movement == Direction.right else -SPEED if movement == Direction.left else 0
		velocity.y = SPEED if movement == Direction.down else -SPEED if movement == Direction.up else 0

		if anim.animation_type == anim.AnimationType.sprint:
			velocity *= 2 * delta
		else:
			velocity *= delta

		# WARNING: DO NOT USE `move_and_slide`, player interaction becomes very buggy
		move_and_collide(velocity * delta)

func update_position(pos: Vector2) -> void:
	position = pos

func update_velocity(vel: Vector2) -> void:
	velocity = vel

func update_rotation_degrees(degrees: float) -> void:
	$Direction.rotation_degrees = degrees

func SetIsInteracting(flag):
	isInteracting = flag
func GetIsInteracting():
	return isInteracting
"

