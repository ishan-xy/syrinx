script/source = "extends TextEdit

var actual_password = \"\"

func _ready():
	# The signal connection is done via the UI, so no need to connect here.
	pass

# Ensure the method name matches the one created by the signal connection via the UI
func _on_TextEdit_text_changed():
	var new_text = text
	if new_text.length() > actual_password.length():
		# Adding new characters
		actual_password += new_text.substr(actual_password.length(), new_text.length() - actual_password.length())
	else:
		# Removing characters
		actual_password = actual_password.substr(0, new_text.length())

	# Create a string of asterisks
	var masked_text = \"\"
	for i in range(actual_password.length()):
		masked_text += \"*\"

	# Replace the displayed text with asterisks
	text = masked_text
"

